{
  "name": "@custom/access-control",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "!dist/**/*.spec.*",
    "!dist/**/*.test.*",
    "dist"
  ],
  "scripts": {
    "generate": "concurrently pnpm:generate:*",
    "dev": "vite dev --port 5174",
    "build": "vite build && npm run package",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint:markup": "markuplint \"**\"",
    "lint:js": "eslint .",
    "lint": "concurrently pnpm:lint:*",
    "test:watch": "vitest",
    "test": "vitest run",
    "format:js": "eslint --fix .",
    "format": "concurrently pnpm:format:*",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package"
  },
  "peerDependencies": {
    "svelte": "5.0.0-next.264"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.8",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.6.4",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
    "@tailwindcss/vite": "4.0.0-alpha.26",
    "dotenv": "^16.4.5",
    "globals": "^15.11.0",
    "publint": "^0.2.11",
    "svelte": "5.0.0-next.264",
    "svelte-check": "^4.0.4",
    "tailwindcss": "4.0.0-alpha.26",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "unplugin-icons": "^0.19.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  }
}
